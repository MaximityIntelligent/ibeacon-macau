    <script>
      $.widget.bridge('uibutton', $.ui.button);
      $(function(){
        $("#menu").load("/user-menu");
        })
    </script>
    <!-- Bootstrap 3.3.5 -->
    <script src="../../bootstrap/js/bootstrap.min.js"></script>
    <script src="../../js/demo.js"></script>
