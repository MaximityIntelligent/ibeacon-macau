
    <div class="wrapper">

      <header class="main-header">
        <!-- Logo -->
        <a href="index2.html" class="logo">
          <!-- mini logo for sidebar mini 50x50 pixels -->
          <span class="logo-mini"><b>M</b>I</span>
          <!-- logo for regular state and mobile devices -->
          <span class="logo-lg"><b>Maximity</b>Ibeacon</span>
        </a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
          <!-- Sidebar toggle button-->
          <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">Toggle navigation</span>
          </a>
              
              <!-- User Account: style can be found in dropdown.less -->
              
        </nav>
      </header>
      <!-- Left side column. contains the logo and sidebar -->
      <aside id="menu" class="main-sidebar">
        <!-- /.sidebar -->
      </aside>

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            App
            <small></small>
          </h1>
          <ol class="breadcrumb">
            <li><a href="/">home</a></li>
            <li><a href="/app">app</a></li>
            <li class="active"><a href="#">read</a></li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">
          <!-- Small boxes (Stat box) -->
          
          <div class="row">
                <!-- left column -->
            <div class="col-md-12">
              <!-- general form elements -->
              <div class="box box-primary">
                <!-- /.box-header -->
                <!-- form start -->
                <p><strong>&nbsp;<%=app.name%></strong></p>
                <p class="text-center"><strong>App Id:</strong> </p>
                <p class="text-center"><%=app.id%></p>
                <p class="text-center"><strong>App Secret:</strong></p>
                <p class="text-center"><%=app.secret%></p>
              </div><!-- /.box -->
              <div class="btn-group"><a class="btn btn-lg success" href="/app-advertisement/new?app=<%=app.id%>">Create Advertisement</a></div>

            </div>
          </div><!-- /.row -->
          <!-- Main row -->
          <div class="row">
                <!-- left column -->
            <div class="col-md-12">
              <!-- general form elements -->
              <div class="box box-primary">


                  <table id="advertisement-table" class="table table-bordered table-hover">
                    <thead>
                      <tr>
                        <th>Title</th>
                        <th>Image Url</th>
                        <th>Text</th>
                        <th>Frequency</th>
                        <th>Valid Date</th>
                        <th>Pick Location</th>
                        <th></th>
                        <th></th>
                        <th></th>
                      </tr>
                    </thead>
                    <tbody>
                      
                      <%
                      var ad;
                      
                      var advertisements = app.has_advertisement;
                      
                      
                      while (advertisements.length) {
                        //code
                        ad = advertisements.pop();
                      %>
                      <tr>
                      <td><!--<a href="/advertisement/read/<%=ad.id%>"></a>--><%=ad.title%></td>
                      <td><%=ad.image_url%></td>
                      <td><%=ad.text%></td>
                      <td><%=ad.frequency%></td>
                      <td>
                        <% if (ad.valid_date!=null) {
                          //code
                          %>
                          <%=ad.valid_date.toUTCString().substring(0, 16)%>
                          <%
                        }
                        %>
                      </td>
                      <td><%=ad.pick_location%></td>
                      <td><a href="/app-advertisement/edit/<%=ad.id%>?r=/app/read/<%=app.id%>">update</a></td>
                      <td><form action="/advertisement/destroy/<%=ad.id%>?r=/app/read/<%=app.id%>" method="post"><input type="submit" value="delete"/></form></td>
                      <td><a href="/advertisement/deploy/<%=ad.id%>">deploy</a></td>
                      </tr>
                      <%
                        
                      }
                      %>
                    </tbody>
                  </table>









              </div><!-- /.box -->
              
            </div>
          </div><!-- /.row -->

        </section><!-- /.content -->
     
     
      </div><!-- /.content-wrapper -->
      <footer class="main-footer">

        <strong>Copyright &copy; 2014-2015 Maximity</strong> All rights reserved.
      </footer>


    </div><!-- ./wrapper -->

