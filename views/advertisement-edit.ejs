
    <div class="wrapper">

      <header class="main-header">
        <!-- Logo -->
        <a href="index2.html" class="logo">
          <!-- mini logo for sidebar mini 50x50 pixels -->
          <span class="logo-mini"><b>M</b>I</span>
          <!-- logo for regular state and mobile devices -->
          <span class="logo-lg"><b>Maximity</b>Ibeacon</span>
        </a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
          <!-- Sidebar toggle button-->
          <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">Toggle navigation</span>
          </a>
              
              <!-- User Account: style can be found in dropdown.less -->
              
        </nav>
      </header>
      <!-- Left side column. contains the logo and sidebar -->
      <aside id="menu" class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <!-- /.sidebar -->
      </aside>

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Advertisement
            <small>edit</small>
          </h1>
          <ol class="breadcrumb">
            <li><a href="/">home</a></li>
            <li><a href="/advertisement">advertisement</a></li>
            <li class="active"><a href="#">edit</a></li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">
          <!-- Small boxes (Stat box) -->
          
          <div class="row">
                <!-- left column -->
            <div class="col-md-6">
              <!-- general form elements -->
              <div class="box box-primary">
                <form role="form" action="/advertisement/<%=advertisement.id%>" method="post">
                  <div class="box-body">
                    <div class="form-group">
                      <label for="exampleInputEmail1">Title</label>
                      <input type="text" name="title" class="form-control" id="exampleInputEmail1" placeholder="" value="<%=advertisement.title%>">
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Image Url</label>
                      <input type="text" name="image_url" class="form-control" id="exampleInputPassword1" placeholder="" value="<%=advertisement.image_url%>">
                    </div>
                          
                    <div class="form-group">
                      <label for="exampleInputPassword1">Text</label>
                      <input type="text" name="text" class="form-control" id="exampleInputPassword1" placeholder="" value="<%=advertisement.text%>">
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Frequency</label>
                      <input type="text" name="frequency" class="form-control" id="exampleInputPassword1" placeholder="" value="<%=advertisement.frequency%>">
                    </div>
                    <div class="form-group">
                    <label>Valid Date</label>
                    <div class="input-group">
                      <div class="input-group-addon">
                        <i class="fa fa-calendar"></i>
                      </div>
                      <input type="text" name="valid_date" class="form-control" data-inputmask="'alias': 'dd/mm/yyyy'" value="<%=advertisement.valid_date%>" data-mask>
                    </div><!-- /.input group -->
                    </div><!-- /.form group -->
                    
                    <div class="form-group">
                      <label for="exampleInputPassword1">Pick Location</label>
                      <input type="text" name="pick_location" class="form-control" id="exampleInputPassword1" placeholder="" value="<%=advertisement.pick_location%>">
                    </div>
                  </div><!-- /.box-body -->

                  <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Update</button>
                  </div>
                </form>
              </div><!-- /.box -->

            </div>
          </div><!-- /.row -->
          <!-- Main row -->
          <div class="row">
            <!-- Left col -->
            
          </div><!-- /.row (main row) -->

        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->
      <footer class="main-footer">

        <strong>Copyright &copy; 2014-2015 Maximity</strong> All rights reserved.
      </footer>

    </div><!-- ./wrapper -->

