
    <div class="wrapper">

      <header class="main-header">
        <!-- Logo -->
        <a href="index2.html" class="logo">
          <!-- mini logo for sidebar mini 50x50 pixels -->
          <span class="logo-mini"><b>M</b>I</span>
          <!-- logo for regular state and mobile devices -->
          <span class="logo-lg"><b>Maximity</b>Ibeacon</span>
        </a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
          <!-- Sidebar toggle button-->
          <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">Toggle navigation</span>
          </a>
              
              <!-- User Account: style can be found in dropdown.less -->
              
        </nav>
      </header>
      <!-- Left side column. contains the logo and sidebar -->
      <aside id="menu" class="main-sidebar">
      </aside>

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Advertisement Deploy
            
          </h1>
          <ol class="breadcrumb">
            <li><a href="/">home</a></li>
            <li><a href="/app">app</a></li>
            <li class="active"><a href="#">advertisement</a></li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">
          <!-- Small boxes (Stat box) -->
          
          <div class="row">
            <div class="col-md-6">
                <div class="box">
                <div class="box-header with-border">
                  <h3 class="box-title">Deployed to devices</h3>
                </div>
                <div class="box-body">
                <table id="advertisement-table" class="table table-bordered table-hover">
                    <thead>
                      <tr>
                        <th>Name</th>
                        <th>Type</th>
                        <th>Location</th>
                        <th></th>
                      </tr>
                    </thead>
                    <tbody>
                      <%
                        var deployment;
                        while (deployments.length) {
                          //code
                          deployment = deployments.pop();
                      %>
                      <tr>
                        <td>
                          <%=deployment.device.name%>
                        </td>
                        <td>
                          <%=deployment.device.type%>
                        </td>
                        <td>
                          <%=deployment.device.location%>
                          
                        </td>
                        <td><form action="/deployment/destroy/<%=deployment.id%>?r=/advertisement/deploy/<%=req.param('id')%>" method="post"><input type="submit" value="delete"/></form></td>
                        
                      </tr>
                      
                      <%
                        }
                      %>
                      

                      
                      
                      
                      
                    </tbody>
                </table>
                </div>
                </div>
            </div>
            
          </div>
          <div class="row">
                <!-- left column -->
            <div class="col-md-6">
              <!-- general form elements -->
              <div class="box box-primary">
                <div class="box-header with-border">
                  <h3 class="box-title">Deploy</h3>
                </div>
                <form role="form" action="/advertisement/deploy/<%=req.param('id')%>" method="post">
                  <div class="box-body">
                    
                    <table id="advertisement-table" class="table table-bordered table-hover">
                    <thead>
                      <tr>
                        <th></th>
                        <th>Name</th>
                        <th>Type</th>
                        <th>Location</th>
                        
                      </tr>
                    </thead>
                    
                    <tbody>
                      <%
                        var device;
                        while (devices.length) {
                          //code
                          device = devices.pop();
                      %>
                      <tr>
                        <td><input type="checkbox" name="deviceId" value="<%=device.id%>"></td>
                        <td><%=device.name%></td>
                        <td><%=device.type%></td>
                        <td><%=device.location%></td>
                        
                      </tr>
                      
                      <%
                        }
                      %>

                      
                      
                      
                    </tbody>
                    </table>
                  
                
                    <div class="form-group">
                      
                                            
                    </div><!-- /.form group -->
                    
                    
                  </div><!-- /.box-body -->

                  <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Deploy</button>
                  </div>
                </form>
              </div><!-- /.box -->

            </div>
          </div><!-- /.row -->
          <!-- Main row -->
          <!-- /.row (main row) -->

        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->
      <footer class="main-footer">

        <strong>Copyright &copy; 2014-2015 Maximity</strong> All rights reserved.
      </footer>


    </div><!-- ./wrapper -->

