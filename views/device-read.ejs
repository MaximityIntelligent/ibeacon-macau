
    <div class="wrapper">

      <header class="main-header">
        <!-- Logo -->
        <a href="index2.html" class="logo">
          <!-- mini logo for sidebar mini 50x50 pixels -->
          <span class="logo-mini"><b>M</b>I</span>
          <!-- logo for regular state and mobile devices -->
          <span class="logo-lg"><b>Maximity</b>Ibeacon</span>
        </a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
          <!-- Sidebar toggle button-->
          <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">Toggle navigation</span>
          </a>
              
              <!-- User Account: style can be found in dropdown.less -->
              
        </nav>
      </header>
      <!-- Left side column. contains the logo and sidebar -->
      <aside id="menu" class="main-sidebar">
      </aside>

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Device
            <small></small>
          </h1>
          <ol class="breadcrumb">
            <li><a href="/">home</a></li>
            <li><a href="/device">device</a></li>
            <li class="active"><a href="#">read</a></li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">
          <!-- Small boxes (Stat box) -->
          
          <div class="row">
                <!-- left column -->
            <div class="col-md-6">
              <!-- general form elements -->
              <div class="box box-primary">
                <!-- /.box-header -->
                <!-- form start -->
                <form role="form">
                  <div class="box-body">
                    <div class="form-group">
                      <label for="exampleInputEmail1">Device Identifier</label>
                      <input type="text" name="name" class="form-control" id="exampleInputEmail1" placeholder="" value="<%=device.name%>">
                    </div>
                    
                    
                    <div class="form-group">
                      <label for="exampleInputPassword1">UUID</label>
                      <input type="text" name="uuid" class="form-control" id="exampleInputPassword1" placeholder="" value="<%=device.uuid%>">
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Major</label>
                      <input type="text" name="major" class="form-control" id="exampleInputPassword1" placeholder="" value="<%=device.major%>">
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Minor</label>
                      <input type="text" name="minor" class="form-control" id="exampleInputPassword1" placeholder="" value="<%=device.minor%>">
                    </div>
                    
                    <div class="form-group">
                      <label for="exampleInputPassword1">State</label>
                      <input type="text" name="state" class="form-control" id="exampleInputPassword1" placeholder="" value="<%=device.state%>">
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">City</label>
                      <input type="text" name="city" class="form-control" id="exampleInputPassword1" placeholder="" value="<%=device.city%>">
                    </div>
                   
                    <div class="form-group">
                      <label for="exampleInputPassword1">Street</label>
                      <input type="text" name="street" class="form-control" id="exampleInputPassword1" placeholder="" value="<%=device.street%>">
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Location</label>
                      <input type="text" name="location" class="form-control" id="exampleInputPassword1" placeholder="" value="<%=device.location%>">
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Location Type</label>
                      <input type="text" name="location" class="form-control" id="exampleInputPassword1" placeholder="" value="<%=device.location_type%>">
                    </div>
                  </div><!-- /.box-body -->

                  </form>
              </div><!-- /.box -->

            </div>
          </div><!-- /.row -->
          <!-- Main row -->
          <div class="row">
            <!-- Left col -->
            
          </div><!-- /.row (main row) -->

        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->
